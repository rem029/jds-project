{
	"name": "@jds-project/backend",
	"description": "Backend for wakra project",
	"version": "1.1.0-alpha.12",
	"main": "src/index.js",
	"private": true,
	"scripts": {
		"start": "yarn clean-logs && ts-node-dev --respawn -r tsconfig-paths/register ./src/index.ts DEBUG=knex:query",
		"build": "tsc -b && resolve-tspaths",
		"build:run": "node ./dist/index.js",
		"dev": "nodemon --exec ts-node-dev -r tsconfig-paths/register ./src/index.ts DEBUG=knex:query",
		"clean-logs": "rimraf ./src/__logs",
		"lint": "eslint src/**/*.ts",
		"migrate-latest": "knex migrate:latest",
		"migrate-rollback": "knex migrate:rollback",
		"db-create": "set PGPASSWORD=123& createdb -U postgres -p 5433 -O postgres jds-db",
		"db-import": "set PGPASSWORD=123& psql -U postgres -p 5433 jds-db < ./sql/jds-db.sql"
	},
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@jds-project/common": "^1.1.0-alpha.11",
		"@types/jsonwebtoken": "^8.5.8",
		"@types/knex": "^0.16.1",
		"@types/node-statsd": "^0.1.3",
		"@types/response-time": "^2.3.5",
		"@types/socket.io": "^3.0.2",
		"bcrypt": "^5.0.1",
		"cors": "^2.8.5",
		"dotenv": "^16.0.0",
		"express": "^4.17.1",
		"form-data": "^4.0.0",
		"jsonwebtoken": "^8.5.1",
		"knex": "^1.0.7",
		"mailgun.js": "^8.2.1",
		"moment-timezone": "^0.5.34",
		"node-fetch": "^3.3.1",
		"node-statsd": "^0.1.1",
		"pg": "^8.10.0",
		"response-time": "^2.3.2",
		"socket.io": "^4.4.1",
		"winston": "^3.7.2"
	},
	"devDependencies": {
		"@types/bcrypt": "^5.0.0",
		"@types/express": "^4.17.13",
		"@types/node": "^17.0.24",
		"@typescript-eslint/eslint-plugin": "^5.19.0",
		"@typescript-eslint/parser": "^5.19.0",
		"eslint": "^8.13.0",
		"nodemon": "^2.0.15",
		"prettier": "^2.6.2",
		"resolve-tspaths": "^0.6.0",
		"rimraf": "^3.0.2",
		"ts-node": "^10.8.1",
		"ts-node-dev": "^2.0.0",
		"tsconfig-paths": "^3.14.0",
		"typescript": "^4.7.4"
	},
	"changelog": {
		"labels": {
			"feature": "New Feature",
			"fix": "Bug Fix",
			"tech": "Tech"
		}
	}
}
